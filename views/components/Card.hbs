<article class='card'>
	<div class='card__urls'>
		<a
			href='{{item.origin}}'
			target='_blank'
			class='card__links'
		>{{item.origin}}</a>
		<a
			href='{{item.shortUrl}}'
			target='_blank'
			class='card__links'
		>{{item.shortUrl}}</a>
	</div>
	<div class='card__btn'>
		<a href='#' class='btn btn--delete'>Eliminar</a>
		<a href='#' class='btn btn--modify'>Editar</a>
		<a href='#' class='btn btn--copy'>Copiar</a>
	</div>
</article>